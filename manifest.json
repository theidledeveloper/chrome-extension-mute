{
    "name": "Mute My Tabs",
    "description": "Mute tabs with a single click",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon-16.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        }
    }
}
